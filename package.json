{
  "name": "myworry-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true",
    "build": "ng build",
    "build:localize": "ng build --localize",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path src/i18n && xliffmerge --profile xliffmerge.json en fr",
    "dev:ssr": "ng run myworry-client:serve-ssr",
    "serve:ssr": "node dist/myworry-client/server/main.js",
    "build:ssr": "ng build --prod && ng run myworry-client:server:production",
    "build-dev:ssr": "ng build && ng run myworry-client:server:dev",
    "build-localized:ssr": "ng build --prod --localize && ng run myworry-client:server:production",
    "prerender": "ng run myworry-client:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "~10.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/localize": "^10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/material-moment-adapter": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/platform-server": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@angular/service-worker": "~10.0.3",
    "@babel/helpers": "^7.10.4",
    "@jefiozie/ngx-aws-deploy": "^1.2.1",
    "@nguniversal/express-engine": "^10.0.1",
    "@swimlane/ngx-charts": "^14.0.0",
    "@yaireo/tagify": "^3.15.3",
    "angularx-social-login": "^2.2.1",
    "bootstrap": "^4.4.1",
    "emojionearea": "^3.4.2",
    "express": "^4.15.2",
    "http-server": "^0.12.3",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ngx-file-drop": "^9.0.1",
    "ngx-socket-io": "^3.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
